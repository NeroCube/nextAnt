    "apollo-server-express": "^1.3.6",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
